<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alien.tryout">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
android
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_developer_mode_black_48dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_developer_mode_black_48dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:screenOrientation="portrait">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UsersProfile"
            android:label="@string/profile"
            android:parentActivityName="com.example.alien.tryout.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.alien.tryout.MainActivity" />
        </activity>

    </application>

</manifest>